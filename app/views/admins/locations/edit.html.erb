<main>
<div class="container">

  <%= render 'admins/adminside', admin: @admin %>

  <h1>勤務地編集</h1>

  <div class="row">
      <div class="location_edit_field">
        <%= form_with model: @location, url: admins_location_path(@location), method: :patch do |f| %>
          <strong>勤務地コード</strong>
          <%= f.text_field :location_code, :size=>"30" %><br>

          <strong>勤務地名</strong>
          <%= f.text_field :location_name, :size=>"30" %>

        <u class="new_botton ml-5">
          <input type="submit" name="commit" value="変更を保存" class="btn btn-success" data-disable-with="Update Genre">
        </u>
　      <% end %>
  </div>

</div>
</main>